apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-deployment-d1
  labels:
    name: fy2011
spec:
  replicas: 3
  selector:
    matchLabels:
      name: fy2011
  template:
    metadata:
      labels:
        name: fy2011
    spec:
      containers:
      - name: nginx
        image: nginx:1.15.4
        ports:
        - containerPort: 80
